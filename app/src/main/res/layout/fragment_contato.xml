<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="20dp"
    tools:context=".ui.contato.ContatoFragment">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="Clientes"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="29dp"
        android:text="Em seu portfólio, após 15 anos de experiência, contabiliza-se como grande fator a qualidade na execução de suas obras, também a fiel manutenção de seus clientes e parceiros, nos credenciando como uma empresa respeitada e sempre indicada por nossos clientes." />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_baseline_arrow_forward_ios_24"
        android:elegantTextHeight="true"
        android:text="Cielo" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_baseline_arrow_forward_ios_24"
        android:elegantTextHeight="true"
        android:text="Brasken" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_baseline_arrow_forward_ios_24"
        android:elegantTextHeight="true"
        android:text="Petrobras" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_baseline_arrow_forward_ios_24"
        android:elegantTextHeight="true"
        android:text="Itaú" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_baseline_arrow_forward_ios_24"
        android:elegantTextHeight="true"
        android:text="Marisa"
        android:textAlignment="viewStart" />
</LinearLayout>